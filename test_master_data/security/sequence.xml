<odoo>
    <data noupdate="1">
         <record id="labor_identification_code" model="ir.sequence">
        <field name="name">HouseMaid Identification Code</field>
        <field name="code">house.maid</field>
        <field name="prefix">HM/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="pro_worker_identification_code" model="ir.sequence">
        <field name="name">ProWorker Identification Code</field>
        <field name="code">pro.worker</field>
        <field name="prefix">PW/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="pro_maid_identification_code" model="ir.sequence">
        <field name="name">ProMaid Identification Code</field>
        <field name="code">pro.maid</field>
        <field name="prefix">PM/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>


    </data>
</odoo>